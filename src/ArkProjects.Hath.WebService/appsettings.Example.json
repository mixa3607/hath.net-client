{
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Serilog.AspNetCore.RequestLoggingMiddleware": "Debug",
        "Microsoft.Hosting.Lifetime": "Verbose",
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "FileManager": {
    //"TempStoragePath": "./tmp",
    //"DownloadsStoragePath": "./downloads",
    "CacheStoragePath": "M:\\HATH\\cache"
  },
  "hath": {
    "ClientId": 10000,
    "ClientKey": "AAAAAAAAAAAAAAAAA",
    "IgnoreRequestInvalidSignature": false,
    "IgnoreRequestInvalidTime": true,
    "IgnoreAddressFromServer": true
  },
  "StatisticsManager": {
    "ClientName": "services.lan",
    "UserId": "000000",
    "GeoipDatabaseFile": "./files/GeoLite2-City.mmdb",
    "GeoipDatabaseType": "City",
    "EnableFilesByCountryCodeMetrics": true,
    "EnableFilesByGeohashMetrics": true
  }
}
